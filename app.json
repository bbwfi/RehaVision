{
  "expo": {
    "name": "RehaVision",
    "slug": "RehaVision",
    "scheme": "rehavision",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_rehavision.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/adaptive-icon_rehavision.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffc107"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDNnFpawNI8OhgRP6gnTGq-Yd5Fo2yjOhg"
        }
      
      },
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon_rehavision.png",
        "backgroundColor": "#ffffff"
      },
      "package": "de.bbwfi.rehavision"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ],
      [
        "expo-location",
        {
          "locationPermission": "Allow $(PRODUCT_NAME) to access your location"
          
        }
      ]
    ]
  }
}
